                                        <ComboBoxItem Name="Mrs" Content="Mrs."/>
                                        <ComboBoxItem Name="Miss" Content="Miss."/>
                                        <ComboBoxItem Name="Ms" Content="Ms."/>
                                        <ComboBoxItem Name="Dr" Content="Dr."/>
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel Style="{StaticResource TableSuccessColumn}">
                                    <TextBlock Name="txtTitleSuccess" HorizontalAlignment="Left"   Text=""  FontSize="16"/>
                                </StackPanel>
                            </StackPanel>

                            <StackPanel Style="{StaticResource TableRowStackPanel}">
